{
  "name": "freesign",
  "private": true,
  "version": "1.0.0",
  "description": "Free and open-source document signing platform",
  "license": "MIT",
  "author": "Nicolas Freiherr von Rosen",
  "type": "module",
  "scripts": {
    "dev": "cp CODE_OF_CONDUCT.md public/ && vite",
    "build": "cp CODE_OF_CONDUCT.md public/ && ROLLUP_SKIP_NODE_RESOLVE=true vite build && cp public/_redirects dist/_redirects",
    "build:dev": "cp CODE_OF_CONDUCT.md public/ && vite build --mode development",
    "start": "npm run build && npm run dev",
    "clean": "rm -rf dist node_modules",
    "clean:start": "npm run clean && npm install && npm run start",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
